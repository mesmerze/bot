- if (template = template_for_current_view)
  = render(template, :org => @org)
- else

  = styles_for :account

  = render 'orgs/title_bar', :org => @org

  = render "comments/new", :commentable => @org
  = render :partial => "shared/timeline", :collection => @timeline

  = hook(:show_org_bottom, self, {entity: @org}) do

    = render "accounts/accounts", :object => @org

    = render :partial => "versions/versions", :locals => {:object => @org}
