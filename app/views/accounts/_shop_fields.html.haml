.nested-fields
  .field
    %table
      %tr
        %td{style: "width: 30px;"}
          = link_to_remove_association "", f, class: "ui-icon ui-icon-minus",
                                              style: 'display:flex; margin:0.5rem 0 0;'
        %td{style: 'width: 200px'}
          = f.text_field :name, style: "width:200px;"
        %td= spacer
        %td
          = f.number_field :num_seats, in: 1..999, style: "width:100px;"
